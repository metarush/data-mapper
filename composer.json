{
    "name": "metarush/data-mapper",
    "type": "library",
    "description": "A generic data access layer for basic CRUD operations. Can act as a layer between database and repositories/services.",
    "keywords": [
        "php",
        "data mapper",
        "pdo"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "metarush/data-mapper contributors",
            "homepage": "https://github.com/metarush/data-mapper/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "MetaRush\\DataMapper\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "atlas/query": "^1.2",
        "phpstan/phpstan": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
